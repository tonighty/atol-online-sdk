{
    "name": "majorov/atol-online-sdk",
    "description": "АТОЛ Онлайн SDK v4",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "psr/log": "^1.1|^2|^3",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/cache": "^6.3",
        "monolog/monolog": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Majorov\\AtolOnlineSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Majorov\\Tests\\AtolOnlineSdk\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vyacheslav Majorov",
            "email": "mjr@feip.co"
        }
    ],
    "minimum-stability": "stable"
